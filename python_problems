
#To find given number is even or odd

def eve():
    if n%2 == 0:
        print("even")
    else:
        print("odd")
n=int(input())
eve()

#to find nth even number
def neven(n):
    return (n-1)*2
n=int(input())
neven1=neven(n)
print(neven1)

