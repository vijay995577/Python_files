# FootballPoints

def Points(wins,looses,draws):
    wins1=wins*3
    looses1=looses*0
    draws1=draws*1
    total=wins1+looses1+draws1
    print(total)
TeamA=Points(4,5,7)

#Invert Keys &Values of a dictionary
#_________________________________________________________________________#

def inversion(dict1):
    l1=[]
    l2=[]
    for x in dict1:
        l1.append(x)
    for x in dict1:
        l2.append(dict1[x])
    print(l1)
    print(l2)
    result=dict(zip(l2,l1))
    print(result)
    print(type(result))
inversion({"A":1,"B":1,"c":23})
